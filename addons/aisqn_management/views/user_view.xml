<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="aisqn_management.form">
      <field name="name">User</field>
      <field name="model">uni.invest.user</field>
      <field name="arch" type="xml">
        <form string="User">
          <sheet>
            <group>
              <field name="username" string="Username"/>
              <field name="email"/>
              <field name="password" widget="password"/>
              <field name="full_name"/>
              <field name="phone"/>
              <field name="invite_aff_code"/>
              <field name="aff_code"/>
              <field name="status"/>
              <field name="role"/>
              <field name="token"/>
              <field name="updated_at"/>
              <field name="updated_last_datetime"/>
              <field name="update_last_by_id"/>
              <field name="created_at" readonly="1"/>
            </group>

          </sheet>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="aisqn_management.list">
      <field name="name">User</field>
      <field name="model">uni.invest.user</field>
      <field name="arch" type="xml">
        <list string="User">
          <field name="username" string="Username"/>
          <field name="email"/>
          <field name="password" widget="password"/>
          <field name="full_name"/>
          <field name="phone"/>
          <field name="invite_aff_code"/>
          <field name="aff_code"/>
          <field name="status"/>
          <field name="role"/>
          <field name="token"/>
          <field name="updated_at"/>
          <field name="updated_last_datetime"/>
          <field name="update_last_by_id"/>
          <field name="created_at" readonly="1"/>
        </list>
      </field>
    </record>
    <record id="action_uni_user" model="ir.actions.act_window">
      <field name="name">Users</field>
      <field name="res_model">uni.invest.user</field>
      <field name="view_mode">list,form</field>
    </record>
